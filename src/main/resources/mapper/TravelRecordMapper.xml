<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.epidemicsurveillance.mapper.TravelRecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.epidemicsurveillance.entity.TravelRecord">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="is_back" property="isBack" />
        <result column="is_delete" property="isDelete" />
        <result column="travel_time" property="travelTime" />
        <result column="back_time" property="backTime" />
        <result column="gmt_create" property="gmtCreate" />
        <result column="gmt_modified" property="gmtModified" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, is_back, is_delete, travel_time, back_time, gmt_create, gmt_modified
    </sql>

</mapper>
